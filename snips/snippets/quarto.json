{
  "fig3": {
    "prefix": ["fig3"],
    "body": [
      "::: {#fig-${1:label} layout=\"[[1, 1], [1]]\"}",
      "",
      "![${2:subcap1}](${3:figure}){#fig-$2}",
      "",
      "![${4:subcap1}](${5:figure}){#fig-$4}",
      "",
      "![${6:subcap1}](${7:figure}){#fig-$6}",
      "",
      "${8:caption}",
      "::::"
    ]
  },
  "fig2": {
    "prefix": ["fig2"],
    "body": [
      "::: {#fig-${1:label} layout=\"[1,1]\"}",
      "",
      "![${2:subcap1}](${3:figure}){#fig-$2}",
      "",
      "![${4:subcap1}](${5:figure}){#fig-$4}",
      "",
      "${6:caption}",
      "::::"
    ]
  },
  "eq": {
    "prefix": ["eq"],
    "body": ["\\$\\$", "$1", "\\$\\$ {#eq-${2: label}}"]
  },
  "Insert Block Equation": {
    "prefix": "$$",
    "body": ["\\$$", "${0}", "\\$$"]
  },
  "py": {
    "prefix": ["py"],
    "body": ["```{python} ", "$1", "```"]
  },
  "rcode": {
    "prefix": ["rcode"],
    "body": ["```{r} ", "$1", "```"]
  },
  "code": {
    "prefix": ["code"],
    "body": ["```{$1} ", "$2", "```", "$0"]
  },
  "pyfig2": {
    "prefix": ["pyfig2"],
    "body": [
      "```{python} ",
      "#| label: fig-$1",
      "#| fig-cap: \"$2\"",
      "#| fig-subcap: ",
      "#|    - \"$3\"",
      "#|    - \"$4\"",
      "#| layout-ncol: 2",
      "",
      "import numpy as np",
      "import matplotlib.pyplot as plt",
      "plt.style.use(['science', 'ieee'])",
      "$5",
      "plt.legend()",
      "plt.show()",
      "",
      "$6",
      "plt.legend()",
      "plt.show()",
      "```"
    ]
  },
  "pyfig1": {
    "prefix": ["pyfig1"],
    "body": [
      "```{python}",
      "#| label: fig-$1",
      "#| fig-cap: \"$2\"",
      "",
      "import numpy as np",
      "import matplotlib.pyplot as plt",
      "",
      "plt.style.use(['science', 'ieee'])",
      "$3",
      "plt.show()",
      "```"
    ]
  },
  "div": {
    "prefix": ["div"],
    "body": ["::: {$1}", "$0", ":::"]
  },
  "footnote": {
    "prefix": ["footnote"],
    "body": ["[^$1]$0"]
  },
  "figref": {
    "prefix": ["figref"],
    "body": ["@fig-$1"]
  },
  "tblref": {
    "prefix": ["tblref"],
    "body": ["@tbl-$1"]
  },
  "eqnref": {
    "prefix": ["eqnref"],
    "body": ["@eq-$1"]
  },
  "secref": {
    "prefix": ["secref"],
    "body": ["@sec-$1"]
  },
  "crossref": {
    "prefix": ["crossref"],
    "body": [
      "crossref:",
      "  fig-title: Figure",
      "  tbl-title: Table",
      "  title-delim: .",
      "  fig-prefix: Figure",
      "  tbl-prefix: Table",
      "  eq-prefix: Eq."
    ]
  },
  "link": {
    "prefix": ["l"],
    "body": ["[$1]($2)$0"]
  },
  "img": {
    "prefix": ["img"],
    "body": ["![](./img/$1)"]
  },
  "fig": {
    "prefix": ["fig"],
    "body": ["![${1:cap1}](${2:figure}){#fig-$3}"]
  },
  "background image": {
    "prefix": ["bgimg"],
    "body": [
      "{data-background-image=\"./img/$1\"  data-background-position=right  data-background-size=contain}"
    ]
  },
  "background iframe": {
    "prefix": ["bgiframe"],
    "body": ["{data-background-iframe=\"$1\"}"]
  },
  "iframe": {
    "prefix": ["iframe"],
    "body": ["<iframe src=\"$1\" width=\"1200\" height=\"700\"></iframe>"]
  },
  "pl": {
    "prefix": ["pl"],
    "body": ["::: {.pull-left}", "$0", ":::"]
  },
  "pr": {
    "prefix": ["pr"],
    "body": ["::: {.pull-right}", "$0", ":::"]
  },
  "cl": {
    "prefix": ["cl"],
    "body": ["::: {.left-column}", "$0", ":::"]
  },
  "cr": {
    "prefix": ["cr"],
    "body": ["::: {.right-column}", "$0", ":::"]
  },
  "container": {
    "prefix": ["con"],
    "body": ["::: {.container}", "$1", ":::"]
  },
  "col": {
    "prefix": ["col"],
    "body": [":::: {.col}", "$1", "::::"]
  },
  "2col": {
    "prefix": ["2col"],
    "body": [
      "::: {.container}",
      "::: {.col}",
      "$1",
      ":::",
      "::: {.col}",
      "$2",
      ":::",
      ":::"
    ]
  },
  "cols": {
    "prefix": ["cols"],
    "body": [
      "::: {.columns}",
      "::: {.column width=\"$1\"}",
      "$2",
      ":::",
      "::: {.column width=\"$3\"}",
      "$4",
      ":::",
      ":::"
    ]
  },
  "lay": {
    "prefix": ["lay"],
    "body": ["::: {layout=\"[$1]\"}", "$0", ":::"]
  },
  "layout-valign": {
    "prefix": ["valign"],
    "body": ["layout-valign=\"$0\""]
  },
  "callout": {
    "prefix": ["call"],
    "body": ["::: {.callout-$1}", "$0", ":::"]
  },
  "todo": {
    "prefix": ["todo"],
    "body": ["::: {.callout-tip}", "## TODO:", "", "$0", ":::"]
  },
  "collapsed notes": {
    "prefix": ["collapse"],
    "body": ["::: {.callout-note collapse=true}", "$0", ":::"]
  },
  "local visible": {
    "prefix": ["local"],
    "body": ["::: {.content-visible when-profile=\"local\"}", "$0", ":::"]
  },
  "col2": {
    "prefix": ["col2"],
    "body": ["::: {layout-ncol=2}", "$0", ":::"]
  },
  "col3": {
    "prefix": ["col3"],
    "body": ["::: {layout-ncol=3}", "$0", ":::"]
  },
  "absolute": {
    "prefix": ["absolute"],
    "body": ["{.absolute top=$1 left=$2 width=\"$3\" height=\"$4\"}"]
  },
  "width": {
    "prefix": ["width"],
    "body": ["{width=\"$3\"}"]
  },
  "hidden": {
    "prefix": ["hidden"],
    "body": ["{visibility=\"hidden\"}"]
  },
  "uncount": {
    "prefix": ["uncount"],
    "body": ["{visibility=\"uncounted\"}"]
  },
  "align": {
    "prefix": ["align"],
    "body": ["{fig-align=\"center\"}"]
  },
  "fragment": {
    "prefix": ["frag"],
    "body": ["::: {.fragment}", "$0", ":::"]
  },
  "notes": {
    "prefix": ["notes"],
    "body": ["::: {.notes}", "$0", ":::"]
  },
  "solution": {
    "prefix": ["solutoin"],
    "body": ["::: {.solution}", "$0", ":::"]
  },
  "aside": {
    "prefix": ["aside"],
    "body": ["::: {.aside}", "$0", ":::"]
  },
  "alert": {
    "prefix": ["alert"],
    "body": ["[$0]{.alert}"]
  },
  "highlight": {
    "prefix": ["highlight"],
    "body": ["[$0]{.highlight}"]
  },
  "fade": {
    "prefix": ["fade"],
    "body": ["[$0]{.fade}"]
  },
  "br": {
    "prefix": ["br"],
    "body": ["<br/>"]
  },
  "panel-fill": {
    "prefix": ["panel"],
    "body": [".panel-fill"]
  },
  "todo scheduled": {
    "prefix": ["scheduled"],
    "body": ["* [ ] ${1:task} -- scheduled:${2:eom}"]
  },
  "todo due": {
    "prefix": ["due"],
    "body": ["* [ ] ${1:task} -- due:${2:eow}"]
  },
  "todo task": {
    "prefix": ["task"],
    "body": ["* [ ] ${1:task}"]
  },
  "importmarkdown": {
    "prefix": ["importmd"],
    "body": ["from IPython.display import display, Markdown"]
  },
  "pymarkdown": {
    "prefix": ["pymd"],
    "body": ["display(Markdown(\"\"\"", "{$1}", "\"\"\".format($1 = $1)))", ""]
  },
  "begin": {
    "body": "\\begin{${1:something}}\n\t$0\n\\end{$1}",
    "description": "begin{} / end{}",
    "prefix": "begin"
  },
  "eqalign": {
    "body": "\\begin{align}\n\t$0\n\\end{align}",
    "description": "Align",
    "prefix": "eqalign"
  },
  "notation": {
    "body": "$1 &= \\text{$2} \\\\\\",
    "description": "notation of variables in an equation",
    "prefix": "notation"
  },
  "blindtext": {
    "body": "\\blindtext",
    "description": "add blindtext in LaTex",
    "prefix": "blind"
  },
  "bibliography": {
    "body": "bibliography: references.bib",
    "description": "add bibliography yaml",
    "prefix": "bib"
  },
  "diary header": {
    "body": ["---", "title: \"$1\"", "date: \"$2\"", "categories: [$3]", "---"],
    "description": "Add minimal yaml header for diaries",
    "prefix": "diary"
  },
  "music header": {
    "body": [
      "---",
      "title: \"$1\"",
      "date: \"$2\"",
      "singer: \"$3\"",
      "lyricist: \"$4\"",
      "composer: \"$5\"",
      "song-language: \"$6\"",
      "categories: [$7]",
      "---"
    ],
    "description": "Add minimal yaml header for music posts",
    "prefix": "music"
  },
  "letter header": {
    "body": [
      "---",
      "toname: \"$1\"",
      "toaddress: |",
      "    | $2",
      "subject: $3",
      "opening: Dear $4,",
      "spacing: ${6:1.2}",
      "---"
    ],
    "description": "Add minimal yaml header for letter",
    "prefix": "letter"
  },
  "yaml header": {
    "body": ["---", "title: $1", "format: $2", "---"],
    "description": "Add minimal yaml header",
    "prefix": "yml"
  },
  "wrap figure": {
    "body": [
      "\\begin{wrapfigure}{r}{${1:width}\\textwidth}",
      "   \\includegraphics[width=${1:width}\\textwidth]{./img/${2:path}}",
      "   \\caption{${3:cap}}",
      "   \\label{fig-${4:label}}",
      "\\end{wrapfigure}"
    ],
    "description": "Add wrap figure with a caption and a label",
    "prefix": "wrap"
  },
  "def list": {
    "body": ["${1:def}", "  ~ ${2:text}"],
    "description": "compact defination list",
    "prefix": "def"
  },
  "ref a wrap figure": {
    "body": "Fig. \\ref{fig-$1}",
    "description": "refer to a label for a wrap figure",
    "prefix": "@wrap"
  },
  "add comment string": {
    "body": "<!-- $1 -->",
    "description": "add a comment string",
    "prefix": "<"
  },
  "shortcodes": {
    "body": "{{< $1 >}}",
    "description": "add shortcodes",
    "prefix": "shortcodes"
  },
  "video": {
    "body": "{{< video src=\"$1\" >}}",
    "description": "video shortcode",
    "prefix": "video"
  },
  "tabset": {
    "prefix": ["tabset"],
    "body": ["::: {.panel-tabset}", "", "## $1", ":::"]
  }
}
